{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "react": ["./node_modules/@types/react"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@dtos/*": ["./src/dtos/*"],
      "@routes/*": ["./src/routes/*"],
      "@screens/*": ["./src/screens/*"],
      "@storage/*": ["./src/storage/*"],
      "@theme": ["./src/theme"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
